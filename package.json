{
  "name": "vite-react-ssr-typescript",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx server",
    "build": "rm -rf dist && yarn run build:client && yarn run build:server && yarn run preview",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr ./entry-server.tsx",
    "preview": "cross-env NODE_ENV=production tsx server",
    "theme": "chakra-cli tokens ./src/theme/index.ts",
    "theme:watch": "chakra-cli tokens ./src/theme/index.ts --watch"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "framer-motion": "^11.1.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@types/compression": "^1.7.5",
    "@types/express": "4.17.15",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/serve-static": "^1.15.7",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "4.18.2",
    "serve-static": "^1.15.0",
    "tsx": "3.12.1",
    "typescript": "^4.9.3",
    "vite": "^5.2.10",
    "vite-plugin-css-injected-by-js": "^3.5.1"
  }
}
